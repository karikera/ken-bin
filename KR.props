<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <KEN_DIR>$(MSBuildThisFileDirectory)</KEN_DIR>
    <VEM_EXCLUDE>$(KEN_DIR)empolyfill;$(KEN_DIR)KRThird\gles_sdk\include;$(VEM_EXCLUDE)</VEM_EXCLUDE>
  </PropertyGroup>
  <PropertyGroup Condition="'$(PlatformShortName)' == 'x64'">
    <PlatformShortName2>x86-64</PlatformShortName2>
  </PropertyGroup>
  <PropertyGroup Condition="'$(PlatformShortName)' != 'x64'">
    <PlatformShortName2>$(PlatformShortName)</PlatformShortName2>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(SolutionDir)bin\$(PlatformShortName)\$(Configuration)\</OutDir>
    <IntDir>bin\$(PlatformShortName)\$(Configuration)\</IntDir>
    <ExecutablePath>$(OutDir);$(KEN_DIR)batch;$(ExecutablePath)</ExecutablePath>
    <IncludePath>$(KEN_DIR);$(KEN_DIR)KRThird\zext-angle-bin\include;$(KEN_DIR)empolyfill;$(IncludePath)</IncludePath>
    <LibraryPath>$(OutDir);$(KEN_DIR)lib\$(PlatformShortName)\$(Configuration);$(KEN_DIR)KRThird\zext-angle-bin\lib\$(PlatformShortName)\$(Configuration);$(MYSQL_C_DIR)lib;$(LibraryPath)</LibraryPath>
    <LinkIncremental>true</LinkIncremental>
    <NMakeBuildCommandLine>embuild</NMakeBuildCommandLine>
    <NMakeReBuildCommandLine>embuild rebuild</NMakeReBuildCommandLine>
    <NMakeCleanCommandLine>embuild clean</NMakeCleanCommandLine>
    <NMakeOutput>$(OutDir)$(ProjectName).js</NMakeOutput>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)' == 'Release'">
    <LinkIncremental>false</LinkIncremental>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="KEN_DIR">
      <Value>$(KEN_DIR)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
  <ItemDefinitionGroup>
    <Link>
      <LinkTimeCodeGeneration>Default</LinkTimeCodeGeneration>
      <GenerateDebugInformation>true</GenerateDebugInformation>
      <ImageHasSafeExceptionHandlers>false</ImageHasSafeExceptionHandlers>
      <AdditionalDependencies>libEGL.lib;libGLESv2.lib;ws2_32.lib;gdiplus.lib;comctl32.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <LinkStatus>
      </LinkStatus>
    </Link>
    <Lib>
      <TargetMachine>MachineX64</TargetMachine>
    </Lib>
    <ClCompile>
      <WarningLevel>Level3</WarningLevel>
      <Optimization>Disabled</Optimization>
      <CallingConvention>VectorCall</CallingConvention>
      <FloatingPointModel>Fast</FloatingPointModel>
      <EnableEnhancedInstructionSet>NotSet</EnableEnhancedInstructionSet>
      <RuntimeLibrary>MultiThreadedDebugDLL</RuntimeLibrary>
      <SDLCheck>true</SDLCheck>
      <AssemblerListingLocation>$(IntDir)%(RelativeDir)</AssemblerListingLocation>
      <ObjectFileName>$(IntDir)%(RelativeDir)</ObjectFileName>
      <XMLDocumentationFileName>$(IntDir)%(RelativeDir)</XMLDocumentationFileName>
      <MinimalRebuild>true</MinimalRebuild>
    </ClCompile>
    <ResourceCompile>
      <Culture>0x0412</Culture>
    </ResourceCompile>
    <MASM>
      <ObjectFileName>$(IntDir)asm$%(FileName).obj</ObjectFileName>
    </MASM>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)' == 'Debug'">
    <ClCompile>
      <PreprocessorDefinitions>PROJECTRUN;_DEBUG;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)' == 'ReleaseTest'">
    <ClCompile>
      <PreprocessorDefinitions>NDEBUG;PROJECTRUN;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <RuntimeLibrary>MultiThreaded</RuntimeLibrary>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)' == 'Release'">
    <ClCompile>
      <PreprocessorDefinitions>NDEBUG;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <Optimization>MaxSpeed</Optimization>
      <InlineFunctionExpansion>AnySuitable</InlineFunctionExpansion>
      <IntrinsicFunctions>true</IntrinsicFunctions>
      <FavorSizeOrSpeed>Speed</FavorSizeOrSpeed>
      <OmitFramePointers>true</OmitFramePointers>
      <StringPooling>true</StringPooling>
      <WholeProgramOptimization>true</WholeProgramOptimization>
      <RuntimeLibrary>MultiThreaded</RuntimeLibrary>
      <FunctionLevelLinking>true</FunctionLevelLinking>
      <MinimalRebuild>false</MinimalRebuild>
    </ClCompile>
    <Link>
      <OptimizeReferences>true</OptimizeReferences>
      <EnableCOMDATFolding>true</EnableCOMDATFolding>
      <LinkTimeCodeGeneration>UseLinkTimeCodeGeneration</LinkTimeCodeGeneration>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Platform)' != 'emscripten'">
    <ClCompile>
      <PreprocessorDefinitions>WIN32;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <DebugInformationFormat>EditAndContinue</DebugInformationFormat>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Platform)' != 'emscripten' and '$(Configuration)' == 'Release'">
    <ClCompile>
      <DebugInformationFormat>ProgramDatabase</DebugInformationFormat>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Platform)'=='Win32'">
    <ClCompile>
      <CallingConvention>FastCall</CallingConvention>
      <EnableEnhancedInstructionSet>StreamingSIMDExtensions</EnableEnhancedInstructionSet>
      <TreatSpecificWarningsAsErrors>4101;4316;4715;%(TreatSpecificWarningsAsErrors)</TreatSpecificWarningsAsErrors>
      <ExceptionHandling>Async</ExceptionHandling>
    </ClCompile>
    <Lib>
      <TargetMachine>MachineX86</TargetMachine>
    </Lib>
  </ItemDefinitionGroup>
</Project>